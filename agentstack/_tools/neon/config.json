{
    "name": "neon",
    "category": "database",
    "url": "https://github.com/neondatabase/neon",
    "cta": "Create an API key at https://www.neon.tech",
    "env": {
        "NEON_API_KEY": null
    },
    "dependencies": [
        "neon-api>=0.1.5",
        "psycopg2-binary==2.9.10"
    ],
    "tools": {
        "create_database": {
            "actions": ["write"]
        },
        "execute_sql_ddl": {
            "actions": ["execute"]
        },
        "run_sql_query": {
            "actions": ["read", "write"]
        }
    }

}